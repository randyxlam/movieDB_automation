package movieDB.quickstart;

import entities.Movie;

public class MoviesData {

	public static final Movie fakeMovie = new Movie();

	private static void runMoviesData() {
		fakeMovie.id = 24428;
		fakeMovie.title = "The Avengers";
		fakeMovie.original_title = "The Avengers";
		fakeMovie.status = Status.RELEASED;
		fakeMovie.tagline = "Some assembly required.";
		fakeMovie.runtime = 143;
		fakeMovie.revenue = 1519557910;
		fakeMovie.release_date = JSON_STRING_DATE.parse("2012-04-25");
		fakeMovie.original_language = "en";
		fakeMovie.budget = 220000000;
		fakeMovie.imdb_id = "tt0848228";
		fakeMovie.overview = "When an unexpected enemy emerges and threatens global safety and security, Nick Fury, director of the international peacekeeping agency known as S.H.I.E.L.D., finds himself in need of a team to pull the world back from the brink of disaster. Spanning the globe, a daring recruitment effort begins!";
		fakeMovie.homepage = "http://marvel.com/avengers_movie/";
		fakeMovie.adult = false;
	}


}
